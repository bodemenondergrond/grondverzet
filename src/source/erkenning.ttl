### eerste deel komt uit grondboring en association
boring:1965-068140  a              grondboringen:Grondboring;
    prov:qualifiedAssociation      association:association001 .
    #prov:wasAssociatedWith erkenning:boring2021-186067 ;
    #prov:wasGeneratedBy voorafmelding:2021-009917 .

association:association001 a    prov:Association ;
    prov:agent                  agent:agent001 ;
    prov:hadPlan                wettelijkkader:boring2021-186067 ;
    prov:hadRole                rol:boorder .

rol:boorder a prov:Role .

wettelijkkader:boring2021-186067 a  prov:Plan ; # grondboringen:WettelijkKaderBoring
    prov:hadMember                  omgevingsvergunning:boring2021-186067 ; # kan ook aktename zijn ipv vergunning OF (1 van de 6 opties, keuze te maken)
    prov:hadMember                  vlaremvergunning:boring2021-186067 ; # kan ook aktename zijn ipv vergunning OF (1 van de 6 opties, keuze te maken)
    prov:hadMember                  anderwettelijkkader:boring2021-186067 ; # OF (1 van de 6 opties, keuze te maken)
    prov:hadMember                  rechtvaardiging:boring2021-186067 ; # OF (1 van de 6 opties, keuze te maken)
    grondboringen:attestVlarem      true ; # OF (1 van de 6 opties, keuze te maken)
    grondboringen:vlareboNaleving   true ; # (1 van de 6 opties, keuze te maken)
    prov:hadMember                  erkenning:boring2021-186067 . # staat onafhankelijk van de 6 voorgaande opties. Kan altijd los hiervan bestaan.


omgevingsvergunning:boring2021-186067 a prov:Entity ;
