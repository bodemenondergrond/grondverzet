#### Korrelverdeling (meetreeks) observatie-fiche https://www.dov.vlaanderen.be/data/observatie/2022-4033232
# Diameter, Fractie met grotere diameter. Diepte_van en Diepte_tot is fix, net als parameter "Korrelverdeling d.m.v. zeefproef"
# en methode "Zeving" en de feature of interest sam:2017-169209 (monster als parent van de observatie).

### Classes

bodemenondergrond:Meetreeks a rdf:Class ;
    rdfs:subClassOf sosa:Observation ;
    rdfs:label "Een meetreeks is een verzameling van hoog frequente data die als één observatie wordt weergegeven."@nl .


### Properties

bodemenondergrond:onderwerp a rdf:Property, qb:DimensionProperty ;
    rdfs:label         "kubus onderwerp property"@nl ;
    rdfs:subPropertyOf sosa:hasFeatureOfInterest .

bodemenondergrond:diameter a rdf:Property, qb:DimensionProperty ;
    rdfs:label         "kubus diameter property"@nl ;
    rdfs:subPropertyOf sosa:observedProperty .

bodemenondergrond:fractiegroterdandiameter a rdf:Property, qb:DimensionProperty ;
    rdfs:label         "kubus fractie groter dan diameter property"@nl ;
    rdfs:subPropertyOf sosa:observedProperty .


### Dataset

dataset:fractiediameter a qb:DataSet ;
    rdfs:label   "kubus korrelverdeling met diameter en fractiebepaling"@nl ;
    rdfs:comment "Dataset van korrelverdeling met diameter en fractiebepaling"@nl ;
    qb:structure dsd:fractiediameter .


### Data structure definition

dsd:fractiediameter a qb:DataStructureDefinition ;
    rdfs:comment "Datastructuur van een korrelverdeling met diameter en fractiebepaling"@nl ;
    qb:component componentspecification:diameter, componentspecification:fractiegroterdandiameter .


### Componentspecifications

componentspecification:diameter a qb:ComponentSpecification ;
    rdfs:label   "diameter ComponentSpecification"@nl ;
    qb:measure   bodemenondergrond:diameter .

componentspecification:fractiegroterdandiameter a qb:ComponentSpecification ;
    rdfs:label   "fractie groter dan diameter ComponentSpecification"@nl ;
    qb:measure   bodemenondergrond:fractiegroterdandiameter .

# Korrelverdeling: diameter, fractiegroterdandiameter, herkomst, fenomeentijd, resultaattijd, uitvoerder, parameter, methode, opmerkingen en bijlagen (diepte_van en diepte_tot komen uit de sample als featureofinterest).
observation:2022-4033232 a                    sosa:Observation ;
    rdfs:label                        "observatie van een korrelverdeling uit monster 2017-169209"@nl ;
    sosa:usedProcedure                cl-opt:305 ; # komt overeen met "Korrelverdeling dmv zeefproef"
    sosa:resultTime                   "2006-08-30T00:00:00.000"^^xsd:dateTime ; # zelfde als phenomenontime indien niet ingevuld in DOV-fiche
    sosa:phenomenonTime               "2006-08-30T00:00:00.000"^^xsd:dateTime ;
    sosa:observedProperty             obs_prop:obs2022-4033232 ;
    sosa:hasResult                    observationgroup:obs2022-4033232 ;
    sosa:hasFeatureOfInterest         sample:2017-169209 ; # monster waarop observatie werd uitgevoerd
    prov:wasStartedBy                 opdracht:2013-006419 ; # fictieve opdracht hier, want geen aanwezig in de DOV-fiche
    sosa:madeBySensor                 agent:agent003 ; # uitvoerder in de DOV-fiche
    prov:atLocation                   her:obs2022-4033232 .

agent:agent003 a prov:Agent .

attribution:agent003_obs2022-4033232 a prov:Attribution ;
    prov:hadRole cl-rol:uitvoerder ;
    prov:agent agent:agent003 .

cl-rol:uitvoerder a prov:Role .

her:obs2022-4033232 a prov:location ;
        skos:notation "Labo" .

cl-opt:305 a  sosa:Procedure ;
        skos:notation  "Zeving" .

obs_prop:obs2022-4033232  a sosa:ObservableProperty ;
        dcterms:subject cl-par:Korrelverdeling_dmv_zeefproef ;
        ssn:isPropertyOf sample:2017-169209 .

