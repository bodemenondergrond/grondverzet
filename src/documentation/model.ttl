geosparql:Geometry  rdf:type  "rdfs:Resource";
        geosparql:asWKT  "http://www.opengis.net/ont/geosparql#wktLiteral" .

sosa:Sample  rdf:type    "rdfs:Resource";
        sosa:isSampleOf  grondboringbeno:Boorgat;
        grondboringbeno:sampleDiepteTot
                grondboringbeno:Diepte;
        grondboringbeno:sampleDiepteVan
                grondboringbeno:Diepte .

grondboringbeno:Boorgat
        rdf:type               "rdfs:Resource";
        geosparql:hasGeometry  geosparql:Geometry;
        ssn:hasProperty        skos:Concept .

grondboringen:Boor  rdf:type  "rdfs:Resource";
        ssn:implements  sosa:Procedure .

time:Instant  rdf:type      "rdfs:Resource";
        time:inXSDDateTime  "http://www.w3.org/2001/XMLSchema#dateTime" .

skos:Concept  rdf:type  "rdfs:Resource" .

grondboringbeno:Diepte
        rdf:type                    "rdfs:Resource";
        rdf:value                   "http://www.w3.org/2001/XMLSchema#decimal";
        sdmx-attribute:unitMeasure  "rdfs:Resource" .

adms:Identifier  rdf:type  "rdfs:Resource";
        skos:notation  "https://data.bodemenondergrond.vlaanderen.be/id/concept/identificatortype/KBIN-BGD" .

sosa:Procedure  rdf:type  "rdfs:Resource";
        rdfs:seeAlso     "rdfs:Resource";
        skos:exactMatch  sosa:Procedure .

sosa:Sampling  rdf:type            "rdfs:Resource";
        adms:identifier            adms:Identifier;
        sosa:hasFeatureOfInterest  grondboringbeno:Boorgat;
        sosa:hasResult             sosa:Sample;
        sosa:madeByActuator        grondboringen:Boor;
        sosa:phenomenonTime        time:Instant;
        sosa:usedProcedure         sosa:Procedure .
